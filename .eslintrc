{
    "extends": [
        "airbnb",
        "prettier"
    ],
    "parser": "babel-eslint",
    "plugins": [
        "prettier",
        "react"
    ],
    "rules": {
        "global-require": 0,
        "no-restricted-globals": 0,
        "prettier/prettier": [
            "error"
        ],
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ],
    }
}